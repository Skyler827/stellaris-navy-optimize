{
  "compilerOptions": {
    "esModuleInterop": true,
    "resolveJsonModule": true,
    "module": "nodenext",
    "moduleResolution": "nodenext",
    "lib": [
      "esnext"
    ],
    "strict": true,
    "sourceMap": true,
    "target": "esnext",
    "outDir": "../dist"
  },
  "include": [
    "src/**/*"
  ],
  "exclude": [
    "node_modules"
  ]
}
